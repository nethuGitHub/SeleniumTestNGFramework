#<yes, yo --> yes - Report will have name with currenttime +index.html, no - report will be in index.html>
overridereports = no

#<yes,no --> yes - Screenshot will be captured for Passed test case. no -  it wont>
passedstepscreenshot=yes

#<yes,no --> yes - Screenshot will be captured for Failed test case. no -  it wont>
failedstepscreenshot=yes

#<yes,no --> yes - Screenshot will be Skipped for Failed test case. no -  it wont>
skippedstepscreenshot=yes 

#<retry failed test cases - yes or no>
retryfailedtestcases=no

#Send report to Kibana ELK report
